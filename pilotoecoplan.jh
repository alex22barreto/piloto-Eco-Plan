entity Empresa {
	tipoIdent String, 
    identificacion String, 
    razonSocial String, 
    celular Integer, 
    email String, 
    personaContacto String
}
entity Proyecto {
	codigo Integer, 
    nombre String,
    estado String,
    tipo String
}
entity Actividades {
	codigo Integer, 
    nombre String, 
    fecInicio Instant, 
    fecFin Instant, 
    estado String,
    tipo String    
    conProveedor Boolean
}

