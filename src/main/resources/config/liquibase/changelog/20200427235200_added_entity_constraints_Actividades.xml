<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Actividades.
    -->
    <changeSet id="20200427235200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="estado_id"
                                 baseTableName="actividades"
                                 constraintName="fk_actividades_estado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="estado"/>

        <addForeignKeyConstraint baseColumnNames="tipo_id"
                                 baseTableName="actividades"
                                 constraintName="fk_actividades_tipo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_actividad"/>

    </changeSet>
</databaseChangeLog>
